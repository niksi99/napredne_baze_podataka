<h1>Primljeni na</h1>
<label>Organizator: </label>
<div class="container pt-5">
    <ul>
        @foreach(var item in Model){
            @foreach (var i in item.PrijavljeniKorisnici)
            {
                <li>@i.Ime</li>
                <li>@i.Prezime</li>
                <li>@i.KontaktTelefon</li>
                <li>@i.Email</li>
            }
        }
    </ul>
</div>