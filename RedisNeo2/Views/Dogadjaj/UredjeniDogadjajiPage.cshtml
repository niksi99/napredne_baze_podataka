@{
}

<h1>Gett all</h1>

<div class="container pt-5">
    <div class="row">
        @*<div class="col-md-6">
            <a class="btn btn-dark" href="/Author/Add">+ Add more</a>
        </div>*@
    </div>
    <div class="row">
        <div class="col-md-6">
            <h3>Aktuelni Dogadjaji</h3>

            <table  class="table table-stripped table-bordered table-dark">
                <thead>
                    <tr>
                        <th>Ime seta</th>
                        <th>Naziv</th>
                        <th>Predvidjeni Broj ucesnika</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach(var item in Model){
                    <tr>
                        <td>@item.sifraseta</td>
                        <td>@item.imeDogadjaj</td>
                        <td>@item.broj</td>
                    </tr>
                    }
                </tbody>
            </table>
        </div>
    </div>
</div>